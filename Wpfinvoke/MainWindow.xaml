<Window x:Class="Wpfinvoke.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpfinvoke"
        mc:Ignorable="d"
        Title="Calculator" Height="236.45" Width="307.252">
    <Grid Margin="0,0,2,-21">
        <TextBox x:Name="a" HorizontalAlignment="Left" Height="23" Margin="85,55,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="b" HorizontalAlignment="Left" Height="23" Margin="85,105,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="confirmation" Content="OK" HorizontalAlignment="Left" Margin="105,150,0,0" VerticalAlignment="Top" Width="75" Click="okclick"/>
        <Label Content="double a: " HorizontalAlignment="Left" Margin="18,55,0,0" VerticalAlignment="Top"/>
        <Label Content="double b: " HorizontalAlignment="Left" Margin="18,105,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
